{
    "title": "Account Manager",
    "showProgressBar": "top",
    "pages": [
        {
            "name": "main_page",
            "elements": [
                {
                    "type": "html",
                    "name": "account_manager_header",
                    "html": "<h3>Account Manager</h3>"
                },
                {
                    "type": "dropdown",
                    "name": "account_manager_name",
                    "title": "Name (optional)",
                    "choices": [
                        "Sean Morsi",
                        "Antony Tobar",
                        "Sam Gandhi",
                        "Peter Irsilious",
                        "Micheal Rahavi",
                        "Sofia Campo",
                        "Elias Halak",
                        "Benjamin Long",
                        "Danny Cifuentes"
                    ]
                },
                {
                    "type": "html",
                    "name": "separator1",
                    "html": "<hr/>"
                },
                {
                    "type": "html",
                    "name": "deal_info_header",
                    "html": "<h3>Deal Info</h3>"
                },
                {
                    "type": "panel",
                    "name": "deal_info_panel",
                    "elements": [
                        {
                            "type": "dropdown",
                            "name": "transaction_type",
                            "title": "Transaction Type",
                            "choices": [
                                "Purchase",
                                "Refinance"
                            ]
                        },
                        {
                            "type": "dropdown",
                            "name": "loan_type",
                            "title": "Loan Type",
                            "choices": [
                                "Fix-N-Flip",
                                "Buy-N-Hold",
                                "Rehab-N-Hold",
                                "Extension"
                            ],
                            "startWithNewLine": false
                        },
                        {
                            "type": "text",
                            "name": "loan_amount",
                            "title": "Loan Amount",
                            "inputType": "number"
                        },
                        {
                            "type": "text",
                            "name": "loan_term",
                            "title": "Loan Term (Months)",
                            "inputType": "number",
                            "startWithNewLine": false
                        },
                        {
                            "type": "dropdown",
                            "name": "property_type",
                            "title": "Property Type",
                            "choices": [
                                "Residential",
                                "Commercial"
                            ]
                        },
                        {
                            "type": "dropdown",
                            "name": "property_subtype",
                            "title": "Property Sub-Type",
                            "choices": [
                                "Single-Family Residence",
                                "Condo",
                                "Townhouse",
                                "2-4 Units",
                                "Land",
                                "Single-Family Residence with ADU",
                                "Manufactured Single-Family Residence"
                            ],
                            "startWithNewLine": false
                        },
                        {
                            "type": "radiogroup",
                            "name": "important_deal_aspect",
                            "title": "What is most important for this deal? (optional)",
                            "choices": [
                                "Highest Leverage",
                                "Lowest Interest Rates",
                                "Lowest Fees"
                            ],
                            "colCount": 3
                        }
                    ],
                    "innerIndent": 1
                },
                {
                    "type": "html",
                    "name": "deal_details_header",
                    "visibleIf": "{transaction_type} notempty",
                    "html": "<h3>Deal Details</h3><hr/>"
                },
                {
                    "type": "panel",
                    "name": "deal_details_panel",
                    "visibleIf": "{transaction_type} notempty",
                    "elements": [
                        {
                            "type": "text",
                            "name": "purchase_price",
                            "title": "Purchase Price (optional)",
                            "inputType": "number"
                        },
                        {
                            "type": "text",
                            "name": "after_repair_value",
                            "title": "After Repair Value (ARV) (optional)",
                            "inputType": "number"
                        },
                        {
                            "type": "text",
                            "name": "construction_budget",
                            "title": "Construction Budget (optional)",
                            "inputType": "number",
                            "startWithNewLine": false
                        },
                        {
                            "type": "text",
                            "name": "down_payment",
                            "title": "Down Payment (optional)",
                            "inputType": "number"
                        },
                        {
                            "type": "dropdown",
                            "name": "property_under_contract",
                            "title": "Is the property under contract? (optional)",
                            "choices": [
                                "Yes",
                                "No"
                            ]
                        },
                        {
                            "type": "text",
                            "name": "estimated_closing_date",
                            "title": "Estimated Closing Date (optional)",
                            "inputType": "date",
                            "visibleIf": "{property_under_contract} = 'Yes'",
                            "startWithNewLine": false
                        },
                        {
                            "type": "comment",
                            "name": "exit_strategy",
                            "title": "Borrower's Exit Strategy (optional)"
                        },
                        {
                            "type": "comment",
                            "name": "more_scenario_info",
                            "title": "More Scenario Info (optional)"
                        }
                    ]
                },
                {
                    "type": "html",
                    "name": "subject_address_header",
                    "visibleIf": "{property_under_contract} notempty",
                    "html": "<h3>Subject Address</h3><hr/>"
                },
                {
                    "type": "panel",
                    "name": "subject_address_panel",
                    "visibleIf": "{property_under_contract} notempty",
                    "elements": [
                        {
                            "type": "text",
                            "name": "street_address",
                            "title": "Street Address (optional)"
                        },
                        {
                            "type": "text",
                            "name": "city",
                            "title": "City (optional)"
                        },
                        {
                            "type": "dropdown",
                            "name": "state",
                            "title": "State (optional)",
                            "choices": [
                                "California"
                            ],
                            "startWithNewLine": false
                        },
                        {
                            "type": "text",
                            "name": "zip_code",
                            "title": "Zip Code (optional)",
                            "startWithNewLine": false
                        }
                    ]
                },
                {
                    "type": "html",
                    "name": "borrower_info_header",
                    "html": "<h3>Borrower Info</h3><hr/>"
                },
                {
                    "type": "panel",
                    "name": "borrower_info_panel",
                    "elements": [
                        {
                            "type": "dropdown",
                            "name": "entity_type",
                            "title": "Entity Type",
                            "choices": [
                                "Individual",
                                "Limited Liability Company",
                                "S Corporation",
                                "C Corporation",
                                "Trust",
                                "TBD"
                            ]
                        },
                        {
                            "type": "text",
                            "name": "entity_legal_name",
                            "title": "Entity Legal Name",
                            "startWithNewLine": false,
                            "visibleIf": "{entity_type} != 'Individual' and {entity_type} != 'TBD'"
                        },
                        {
                            "type": "text",
                            "name": "signer_title",
                            "title": "Signer Title",
                            "visibleIf": "{entity_type} != 'Individual' and {entity_type} != 'TBD'"
                        },
                        {
                            "type": "text",
                            "name": "signer_first_name",
                            "title": "Signer First Name",
                            "startWithNewLine": false,
                            "visibleIf": "{entity_type} != 'TBD'"
                        },
                        {
                            "type": "text",
                            "name": "signer_last_name",
                            "title": "Signer Last Name",
                            "visibleIf": "{entity_type} != 'TBD'"
                        },
                        {
                            "type": "text",
                            "name": "signer_email",
                            "title": "Signer Email Address",
                            "inputType": "email",
                            "startWithNewLine": false,
                            "visibleIf": "{entity_type} != 'TBD'"
                        },
                        {
                            "type": "text",
                            "name": "signer_phone",
                            "title": "Signer Phone Number",
                            "inputType": "tel",
                            "visibleIf": "{entity_type} != 'TBD'"
                        },
                        {
                            "type": "dropdown",
                            "name": "is_foreign_national",
                            "title": "Is the guarantor a foreign national?",
                            "choices": [
                                "Yes",
                                "No"
                            ],
                            "startWithNewLine": false,
                            "visibleIf": "{entity_type} != 'TBD'"
                        },
                        {
                            "type": "dropdown",
                            "name": "credit_score_range",
                            "title": "Guarantor's Credit Score Range",
                            "choices": [
                                "800-850",
                                "740-799",
                                "670-739",
                                "580-669",
                                "300-579",
                                "TBD"
                            ],
                            "startWithNewLine": false,
                            "visibleIf": "{is_foreign_national} = 'No' and {entity_type} != 'TBD'"
                        }
                    ]
                }
            ]
        }
    ]
}
