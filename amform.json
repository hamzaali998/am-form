        },
        {
          "type": "dropdown",
          "name": "account_manager_name",
          "title": "Name (optional)",
          "choices": [
            "Sean Morsi",
            "Antony Tobar",
            "Sam Gandhi",
            "Peter Irsilious",
            "Micheal Rahavi",
            "Sofia Campo",
            "Elias Halak",
            "Benjamin Long",
            "Danny Cifuentes"
          ]
        },
        {
          "type": "html",
          "name": "separator1",
          "html": "<hr/>"
        },
        {
          "type": "html",
          "name": "deal_info_header",
          "html": "<h3>Deal Info</h3>"
        },
        {
          "type": "panel",
          "name": "deal_info_panel",
          "elements": [
            {
              "type": "multipletext",
              "name": "deal_info_row1",
              "items": [
                {
                  "name": "transaction_type",
