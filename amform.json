{
  "title": "Account Manager",
  "showProgressBar": "top",
  "pages": [
    {
      "name": "main_page",
      "elements": [
        {
          "type": "html",
          "name": "account_manager_header",
          "html": "<h3>Account Manager</h3>"
        },
        {
          "type": "dropdown",
          "name": "account_manager_name",
          "title": "Name (optional)",
          "choices": [
            "Sean Morsi", "Antony Tobar", "Sam Gandhi", "Peter Irsilious", "Micheal Rahavi",
            "Sofia Campo", "Elias Halak", "Benjamin Long", "Danny Cifuentes"
          ]
        },
        {
          "type": "html",
          "name": "separator1",
          "html": "<hr/>"
        },
        {
          "type": "html",
          "name": "deal_info_header",
          "html": "<h3>Deal Info</h3>"
        },
        {
          "type": "panel",
          "name": "deal_info_panel",
          "elements": [
            {
              "type": "dropdown",
              "name": "transaction_type",
              "title": "Transaction Type",
              "choices": ["Purchase", "Refinance"]
             
            },
            {
              "type": "dropdown",
              "name": "loan_type",
              "title": "Loan Type",
              "choices": ["Fix-N-Flip", "Buy-N-Hold", "Rehab-N-Hold", "Extension"],
               "startWithNewLine": false
            },
            {
              "type": "text",
              "name": "loan_amount",
              "title": "Loan Amount",
              "inputType": "number"
             
            },
            {
              "type": "text",
              "name": "loan_term",
              "title": "Loan Term (Months)",
              "inputType": "number",
               "startWithNewLine": false
            },
            {
              "type": "dropdown",
              "name": "property_type",
              "title": "Property Type",
              "choices": ["Residential", "Commercial"]
              
            },
            {
              "type": "dropdown",
              "name": "property_subtype",
              "title": "Property Sub-Type",
              "choices": [
                "Single-Family Residence", "Condo", "Townhouse", "2-4 Units",
                "Land", "Single-Family Residence with ADU", "Manufactured Single-Family Residence"
              ],
              "startWithNewLine": false
            },
            {
              "type": "radiogroup",
              "name": "important_deal_aspect",
              "title": "What is most important for this deal? (optional)",
              "choices": ["Highest Leverage", "Lowest Interest Rates", "Lowest Fees"],
              "colCount": 3
            }
          ],
          "innerIndent": 1
        }
      ]
    }
  ]
}
