                },
                {
                  "name": "loan_type",
                  "title": "Loan Type"
                }
              ]
            },
            {
              "type": "multipletext",
              "name": "deal_info_row2",
              "items": [
                {
                  "name": "loan_amount",
                  "title": "Loan Amount"
                },
                {
                  "name": "loan_term",
                  "title": "Loan Term"
                }
              ]
            },
            {
              "type": "multipletext",
              "name": "deal_info_row3",
              "items": [
                {
                  "name": "property_type",
                  "title": "Property Type"
                },
                {
                  "name": "property_subtype",
                  "title": "Property Sub-Type"
                }
              ]
            },
            {
              "type": "radiogroup",
              "name": "important_deal_aspect",
              "title": "What is most important for this deal? (optional)",
              "choices": [
                "Highest Leverage",
                "Lowest Interest Rates",
                "Lowest Fees"
